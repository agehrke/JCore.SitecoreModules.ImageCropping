<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <fieldTypes>
      <fieldType name="ExtendedImage" type="Sitecore.SharedModules.ImageCropping.Data.Fields.ExtendedImageField,Sitecore.SharedModules.ImageCropping" />
    </fieldTypes>
    <pipelines>
      <getMediaStream>
        <processor type="Sitecore.SharedModules.ImageCropping.Resources.Media.CropProcessor, Sitecore.SharedModules.ImageCropping" patch:before="*[@type='Sitecore.Resources.Media.ThumbnailProcessor, Sitecore.Kernel']" />
      </getMediaStream>
    </pipelines>
  </sitecore>
</configuration>